    "redux-saga": "^0.15.6",
    "styled-components": "^3.2.6",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.5.1",
    "whatwg-fetch": "^2.0.3",
    "yarn-or-npm": "^2.0.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
