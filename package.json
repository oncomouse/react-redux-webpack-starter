{
  "name": "react-redux-webpack-starter",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/oncomouse/react-redux-webpack-starter.git"
  },
  "main": "app/index.jsx",
  "title": "Sample React-Redux App",
  "scripts": {
    "cover": "cross-env BABEL_ENV=test NODE_ENV=coverage nyc -r text -r lcov yon run mocha",
    "mocha:watch": "yon run mocha -- --watch",
    "mocha": "mocha-webpack test.prepare.js \"app/**/*.spec.js\" \"app/**/*.spec.jsx\"",
    "test": "yon run lint && yon run mocha",
    "start": "webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack",
    "analyze": "cross-env ANALYZE=1 NODE_ENV=production webpack",
    "lint": "eslint --ext .js --ext .jsx app/"
  },
  "author": "Andrew Pilsch <apilsch@tamu.edu> (https://andrew.pilsch.com)",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
