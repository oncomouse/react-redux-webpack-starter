    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.0",
    "mocha-webpack": "^1.1.0",
    "nyc": "^11.4.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.0",
    "sinon": "^4.2.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.11.0",
    "webpack-node-externals": "^1.6.0"
  },
  "nyc": {
    "include": [
      "app/**/*.js",
      "app/**/*.jsx"
